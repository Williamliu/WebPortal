@section scripts {
    <script>
        var viewData = new WLIU.ViewData();
        var app = angular.module("myApp", ["wliu_database"]);

        app.controller("mycontrl", function ($scope) {
            viewData.LinkScope($scope);
        });
        viewData.Init("/Admin/api/Setting/InitProvince");
    </script>
}
<div ng-app="myApp" ng-controller="mycontrl">
    <ng-form>
        <div wliu table grow shrink style="margin-top:12px;">
            <div row>
                <div cell small right>
                    <filter.label h1 fit right db="ViewData.database" tb="Province" col="CountryFilter"></filter.label>
                </div>
                <div cell large>
                    <filter.linkselect h1 style="min-width:320px;" db="ViewData.database" tb="Province" col="CountryFilter"></filter.linkselect>
                </div>
                <div cell small>
                </div>
            </div>
        </div>
        <div wliu table grow shrink style="margin-top:12px;">
            <div row>
                <div cell style="background-color:#dddddd;">
                    <wliu.navi db="ViewData.database" tb="Province"></wliu.navi>
                </div>
            </div>
            <div row>
                <div head fixed style="width:40px;"></div>
                <div head medium><wliu.head db="ViewData.database" tb="Province" col="TitleEN"></wliu.head></div>
                <div head large><wliu.head db="ViewData.database" tb="Province" col="DetailEN"></wliu.head></div>
                <div head medium><wliu.head db="ViewData.database" tb="Province" col="TitleCN"></wliu.head></div>
                <div head large><wliu.head db="ViewData.database" tb="Province" col="DetailCN"></wliu.head></div>
                <div head fixed style="width:60px;"><wliu.head db="ViewData.database" tb="Province" col="Sort"></wliu.head></div>
                <div head fixed style="width:60px;"><wliu.head db="ViewData.database" tb="Province" col="Active"></wliu.head></div>
                <div head fixed style="width:56px;">
                    <wliu.hicon db="ViewData.database" tb="Province" icons="add,save,cancel"></wliu.hicon>
                </div>
            </div>
            <div row ng-repeat="row in ViewData.database.tables.Province.rows">
                <div cell center fixed style="width:40px;">
                    <wliu.rowstatus db="ViewData.database" tb="Province" guid="{{row.guid}}"></wliu.rowstatus>
                </div>
                <div cell medium>
                    <wliu.textbox db="ViewData.database" tb="Province" guid="{{row.guid}}" col="TitleEN"></wliu.textbox>
                </div>
                <div cell large>
                    <wliu.textbox db="ViewData.database" tb="Province" guid="{{row.guid}}" col="DetailEN"></wliu.textbox>
                </div>
                <div cell medium>
                    <wliu.textbox db="ViewData.database" tb="Province" guid="{{row.guid}}" col="TitleCN"></wliu.textbox>
                </div>
                <div cell large>
                    <wliu.textbox db="ViewData.database" tb="Province" guid="{{row.guid}}" col="DetailCN"></wliu.textbox>
                </div>
                <div cell center fixed style="width:60px;">
                    <wliu.textbox db="ViewData.database" tb="Province" guid="{{row.guid}}" col="Sort"></wliu.textbox>
                </div>
                <div cell center fixed style="width:60px;">
                    <wliu.bool db="ViewData.database" tb="Province" guid="{{row.guid}}" col="Active"></wliu.bool>
                </div>
                <div cell fixed style="width:56px;">
                    <wliu.bicon db="ViewData.database" tb="Province" guid="{{row.guid}}" icons="save,cancel"></wliu.bicon>
                </div>
            </div>
        </div>
    </ng-form>

    <table.error></table.error>
    <form.error></form.error>

    <wliu.tooltip></wliu.tooltip>
    <wliu.hint></wliu.hint>
    <wliu.loadding oper="get"></wliu.loadding>
    <wliu.loadding oper="save"></wliu.loadding>
</div>
